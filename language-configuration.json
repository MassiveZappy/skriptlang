{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["{", "}"],
    ["(", ")"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "\"", "close": "\"" }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["(", ")"],
    ["[", "]"],
    ["\"", "\""]
  ],
  "wordPattern": "[a-zA-Z][a-zA-Z0-9_\\-]*",
  "indentationRules": {
    "increaseIndentPattern": ":\\s*$",
    "decreaseIndentPattern": "^\\s*(?:else|catch)\\s*:"
  }
}
