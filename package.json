{
  "name": "tree-sitter-skriptlang",
  "version": "0.1.0",
  "description": "Skript grammar for Tree-sitter",
  "main": "bindings/node",
  "scripts": {
    "build": "tree-sitter generate",
    "test": "tree-sitter test"
  },
  "keywords": [
    "parser",
    "lexer",
    "skript",
    "language",
    "tree-sitter",
    "grammar",
    "minecraft"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "tree-sitter": [
    {
      "scope": "source.skript",
      "file-types": [
        "sk"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "indents": [
        "queries/indents.scm"
      ]
    }
  ]
}
