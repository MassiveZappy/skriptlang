{
  "name": "skriptlang",
  "version": "0.1.0",
  "description": "Skript language support for Zed Editor",
  "repository": "https://github.com/user/tree-sitter-skriptlang",
  "author": "",
  "grammars": {
    "skriptlang": "grammars/skriptlang"
  },
  "language_configurations": {
    "skriptlang": "language-configuration.json"
  },
  "languages": {
    "skriptlang": {
      "name": "Skript",
      "grammar": "skriptlang",
      "language_configuration": "skriptlang",
      "extensions": [".sk"],
      "first_line_pattern": "# (Skript|skript|SKRIPT)"
    }
  },
  "categories": ["languages"]
}
